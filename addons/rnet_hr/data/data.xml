<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Sequence for hr.employee.review -->
        <record id="seq_hr_employee_review" model="ir.sequence">
            <field name="name">Employee Review</field>
            <field name="code">hr.employee.review</field>
            <field name="prefix">REVIEW/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- cron update work_schedule_id di attendance for older record not set yet -->
        <record id="ir_cron_retrofill_attendance" model="ir.cron">
            <field name="name">Retrofill Attendance Work Schedule</field>
            <field name="model_id" ref="hr_attendance.model_hr_attendance"/>
            <field name="state">code</field>
            <field name="code">model.retrofill_work_schedule()</field>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">1</field>
        </record>

    </data>
</odoo>